generator client {
  provider = "prisma-client-js"
  output   = "../../.generated/prisma/accounts"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_ACCOUNTS_URL")
}

model profiles {
  first_name String @db.VarChar(32)
  last_name  String @db.VarChar(32)
  id         String @id(map: "profiles_pk") @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
}
