generator client {
  provider = "prisma-client-js"
  output   = "../../.generated/prisma/cars"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_CARS_URL")
}

model manufacture {
  id       String    @id(map: "manufacture_pk") @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  make     String    @db.VarChar(32)
  model    String    @db.VarChar(32)
  year     Int
  variants variants?
}

model variants {
  id          String      @id(map: "variants_pk") @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  mileage     Int
  car_id      String      @unique(map: "variants_unique") @db.Uuid
  manufacture manufacture @relation(fields: [car_id], references: [id], onDelete: Cascade, map: "variants_manufacture_fk")
}
